{
	"name": "spotify-api-ts",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/index.js",
	"scripts": {
		"build": "rimraf ./dist && npm run lint && tsc",
		"format": "prettier --write **/*ts",
		"lint": "eslint --fix **/*.ts",
		"prepublish": "npm run build",
		"docs": "typedoc --out docs",
		"precommit": "lint-staged",
		"start:server": "node ./dist/serverRunner",
		"main": "node ./dist/main",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.ts": [
			"npm run format",
			"npm run lint",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"keywords": [
		"Typescript",
		"Spotify"
	],
	"author": "Rajdeep Dey",
	"license": "ISC",
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^18.15.3",
		"@types/spotify-web-api-node": "^5.0.7",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"dotenv": "^16.0.3",
		"esdoc": "^1.0.4",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "8.7.0",
		"eslint-plugin-prettier": "4.2.1",
		"express": "^4.18.2",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"log4js": "^6.3.0",
		"moment": "^2.29.4",
		"prettier": "^2.8.5",
		"rimraf": "^4.4.0",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.28",
		"typescript": "^5.0.2"
	},
	"dependencies": {
		"spotify-web-api-node": "^5.0.2"
	}
}
